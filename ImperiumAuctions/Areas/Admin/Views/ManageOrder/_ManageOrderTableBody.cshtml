@model IEnumerable<AuctionOrder>

@for (int i = 0; i < Model.Count(); i++)
{
    <tr>
        <td>@(i + 1)</td>
        <td>@Model.ElementAt(i).Product?.Title</td>
        <td>@Model.ElementAt(i).FinalBidPrice</td>
        <td>@(Model.ElementAt(i).PaymentDueDate.ToString("MMM dd, yyyy h:mm tt") + " PKT")</td>
        <td>
            @(Model.ElementAt(i).PaymentDate != DateTime.MinValue
                ? Model.ElementAt(i).PaymentDate.ToString("MMM dd, yyyy h:mm tt") + " PKT"
                : "")
        </td>
        <td>@Model.ElementAt(i).PaymentStatus</td>
        <td>@Model.ElementAt(i).DeliveryStatus</td>
        <td>
            <div class="btn-group w-100 flex-column gap-2" role="group">
                <a class="btn bg-gradient-right btn-hover rounded statusShipped" data-order-id="@Model.ElementAt(i).OrderId">
                    @StaticValues.DeliveryStatusShipped
                </a>

                <a class="btn bg-gradient-right btn-hover rounded statusDelivered" data-order-id="@Model.ElementAt(i).OrderId">
                    @StaticValues.DeliveryStatusDelivered
                </a>
            </div>
        </td>
    </tr>
}